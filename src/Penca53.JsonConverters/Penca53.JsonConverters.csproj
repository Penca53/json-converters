<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net48;net5.0;netstandard2.0</TargetFrameworks>
    <Version>1.0.1</Version>
    <AssemblyTitle>Penca53.JsonConverters</AssemblyTitle>
    <Description>A set of Json Converters which allow to serialize and deserialize variables without the limitation of only public variables present at the current state of `System.Text.Json`.</Description>
    <PackageTags>json;converter;c#</PackageTags>
    <Authors>Penca53</Authors>
    <RepositoryUrl>https://github.com/Penca53/json-converters</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/Penca53/json-converters/blob/main/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright Â© 2021 Penca53</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>
</Project>
